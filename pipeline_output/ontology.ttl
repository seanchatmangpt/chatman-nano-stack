@prefix : <http://cns.io/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

: a owl:Ontology ;
    rdfs:label "CNS Working Pipeline Ontology" ;
    rdfs:comment "Generated by working pipeline demonstrating all component connections" .


:DataStream a owl:Class ;
    rdfs:label "DataStream" ;
    rdfs:comment "Attributes: id, source, format, rate" .

:Processor a owl:Class ;
    rdfs:label "Processor" ;
    rdfs:comment "Attributes: id, type, config, capacity" .

:Pattern a owl:Class ;
    rdfs:label "Pattern" ;
    rdfs:comment "Attributes: id, expression, severity" .

:Alert a owl:Class ;
    rdfs:label "Alert" ;
    rdfs:comment "Attributes: id, message, timestamp, priority" .

:processes a owl:ObjectProperty ;
    rdfs:domain :Processor ;
    rdfs:range :DataStream .

:detects a owl:ObjectProperty ;
    rdfs:domain :Processor ;
    rdfs:range :Pattern .

:triggers a owl:ObjectProperty ;
    rdfs:domain :Pattern ;
    rdfs:range :Alert .

:DataStreamShape a sh:NodeShape ;
    sh:targetClass :DataStream ;
    rdfs:label "DataStream Shape" .

:ProcessorShape a sh:NodeShape ;
    sh:targetClass :Processor ;
    rdfs:label "Processor Shape" .

:PatternShape a sh:NodeShape ;
    sh:targetClass :Pattern ;
    rdfs:label "Pattern Shape" .

:AlertShape a sh:NodeShape ;
    sh:targetClass :Alert ;
    rdfs:label "Alert Shape" .
